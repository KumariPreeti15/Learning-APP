  }

  @property() myProp = 'Hello';

  render() {
    return html`
      <omni-style>

      </omni-style>
    `;
  }
}

OmniElement.register('{{getTagName name}}', {{getName name}});

declare global {
  interface HTMLElementTagNameMap {
    '{{getTagName name}}', {{getName name}};
  }
}
