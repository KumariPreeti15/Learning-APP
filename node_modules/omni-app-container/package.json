{
  "name": "omni-app-container",
  "version": "4.3.7",
  "description": "Webcomponent omni-app-container for Omni web applications following open-wc guidelines",
  "author": "Annalect",
  "license": "UNLICENSED",
  "exports": {
    ".": "./index.js",
    "./omni-transform-plugin.js": "./omni-transform-plugin.js",
    "./proxies.js": "./proxies.js"
  },
  "main": "./index.js",
  "type": "module",
  "engines": {
    "node": ">=14.20.0"
  },
  "scripts": {
    "analyze": "cem analyze --litelement",
    "start": "web-dev-server",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint --ext .js,.mjs,.html --ignore-path .gitignore .; exit 0;",
    "lint:prettier": "prettier --check --ignore-path .gitignore .; exit 0;",
    "format": "npm run format:prettier && npm run format:eslint",
    "format:eslint": "eslint --ext .js,.mjs,.html --fix --ignore-path .gitignore .; exit 0;",
    "format:prettier": "prettier --write --ignore-path .gitignore .; exit 0;",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "test": "web-test-runner --coverage",
    "test:watch": "web-test-runner --watch"
  },
  "dependencies": {
    "@vaadin/router": "^1.7.4",
    "construct-style-sheets-polyfill": "^3.1.0",
    "omni-ui": "bitbucket:annalect/omni-ui#semver:^7.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^3.1.6",
    "@web/dev-server": "^0.1.35",
    "@web/test-runner": "^0.13.22",
    "auto-changelog": "^2.4.0",
    "browserslist": "^4.21.10",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-compat": "^4.0.2",
    "lint-staged": "^15.2.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-entrypoint-hashmanifest": "^0.1.2",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-resolve-id": "^1.0.2",
    "rollup-plugin-terser": "^7.0.2"
  },
  "customElements": "custom-elements.json",
  "lint-staged": {
    "*.{js,ts,mjs,html}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,json,md,yaml}": "prettier --write"
  },
  "vaadin": {
    "disableUsageStatistics": true
  }
}
